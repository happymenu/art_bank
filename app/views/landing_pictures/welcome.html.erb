<html>
<head>
<title>中国艺术银行</title>

<%= stylesheet_link_tag "application", :media => "all"%>
<%= javascript_include_tag "application" %>
</head>
<body style='background-image: url(<%= asset_path "logo.jpg"%>); background-position:center;'>
<div id="click_div"  style="width:100%;height:100%;" align='center'>
  <img id="img" src="<%= asset_path 'logo_star.png' %>" width="256" height="256" />
</div>

<script type="text/javascript" src="http://qd.baidupcs.com/file/511453594d5fa4e24d85ab0046929c6a?bkt=p2-qd-87&fid=2049887711-250528-422677078583712&time=1417594610&sign=FDTAXERLB-DCb740ccc5511e5e8fedcff06b081203-G6bow2mDT%2BUKwRDVnXWaEVeHuC0%3D&to=qb&fm=Qin,B,U,nc&newver=1&newfm=1&flow_ver=3&sl=81723462&expires=8h&rt=sh&r=908186876&mlogid=3070552795&vuk=2049887711&vbdid=3464176833&fin=jQueryRotate.js&fn=jQueryRotate.js"></script>
<script>
$(document).ready(function(){

    $("#click_div").rotate({
         bind:
              {
                      click: function(){
                      $('#img').rotate({ angle:0,animateTo:360,easing: $.easing.easeInOutExpo });
                      window.location="landing_pictures";

                                          }
             }

       });




    });
</script>
</body>

</html>
