<h1>Listing landing_pictures</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Logo</th>
      <th>Landing picture type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @landing_pictures.each do |landing_picture| %>
      <tr>
        <td><%= landing_picture.name %></td>
        <td><%= landing_picture.logo %></td>
        <td><%= landing_picture.landing_picture_type %></td>
        <td><%= link_to 'Show', landing_picture %></td>
        <td><%= link_to 'Edit', edit_landing_picture_path(landing_picture) %></td>
        <td><%= link_to 'Destroy', landing_picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Landing picture', new_landing_picture_path %>
